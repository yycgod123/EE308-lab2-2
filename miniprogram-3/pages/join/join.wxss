/* pages/group_game/group_game.wxss */
.bgImg {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
}
.other-widget{
    position: absolute;
}
/*Create room interface*/
.container{
  width: 100%;
  height: 1200rpx;
  margin: 0;
  padding: 0;
  position: relative;
}
image{
  position: absolute;
  width: 100%;
  height: 1220rpx;
}
.up{
  position: absolute;
  z-index: 1;
}
.input_ID{  /*input room ID*/
    position: fixed;
    width: 90%;
    margin: 0 5%;
    display: flex;
    flex-direction: row;
    align-items: center;
    top: 20rpx;
}

.input{ /*drop-down box */
  position: fixed;
  margin: 0 5%;
  top: 60rpx;
  left: 210rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 200rpx;
  width: 240rpx;
  height: 65rpx;
  font-size: small;
  font-weight: 400;
  border-radius: 10px;
  color: rgb(250, 2, 2);
  border: 4rpx solid rgb(250, 2, 2);
  
}

.in{    /*ok button*/
  position: fixed;
  margin: 0 5%;
  top: 290rpx;
  left: 260rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 200rpx;
  width: 150rpx;
  height: 60rpx;
  font-size:medium;
  font-weight: 400;
  border-radius: 30px;
  background-color: var(--redColor);
  color: var(--yellowColor);
  border-width: 60rpx;
  border-color: var(--yellowColor);
}

.tip{ /*Input prompt*/
  font-size: 35rpx;
  color: rgb(174, 248, 0);
  position: fixed;
  margin: 0 5%;
  top: 400px;
  left: 10rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  margin-top: 200rpx;
}

